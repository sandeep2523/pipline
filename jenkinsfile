 pipeline {
    agent none
    stages {
        stage('ALL') {
        parallel {
           stage('OWN-BUILD') {
            agent { label 'slave1' }
            git branch: 'main', url: 'https://github.com/sandeep2523/c-project-demo.git'
          }
          stage('OWN-DEPLOY') {
            agent { label 'master' }
            echo "hello"
          }
        }
        }
    }
}
